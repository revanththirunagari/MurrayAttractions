<p id="notice"><%= notice %></p>

<h1>Listing Attractionsdbs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Rating</th>
      <th>Entry</th>
      <th>Web</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attractionsdbs.each do |attractionsdb| %>
      <tr>
        <td><%= attractionsdb.name %></td>
        <td><%= attractionsdb.description %></td>
        <td><%= attractionsdb.rating %></td>
        <td><%= attractionsdb.entry %></td>
        <td><%= attractionsdb.web %></td>
        <td><%= link_to 'Show', attractionsdb %></td>
        <td><%= link_to 'Edit', edit_attractionsdb_path(attractionsdb) %></td>
        <td><%= link_to 'Destroy', attractionsdb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attractionsdb', new_attractionsdb_path %>
